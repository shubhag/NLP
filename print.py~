if __name__ == '__main__':
	f = open('input.txt', 'r')
	wr = open('outputa.txt', 'w')
	dictCount = dict()
	for line in f:
		decodedLine = line.decode('utf-8')
		for word in decodedLine:
			if word >= u'\u0900' and word <= u'\u097F' :
				encodedWord = word.encode('utf-8')
				wr.write(encodedWord)

	f.close()
	wr.close()
